import turtle
t = turtle.Turtle()
t.shape("turtle")
t.fillcolor("purple")

t.forward(50)
t.right(90)
t.penup()
t.forward(50)
t.right(90)
t.pendown()
t.forward(50)
t.right(90)
t.penup()
t.forward(50)
t.pendown()

t.fillcolor("red")
t.forward(50)
t.right(90)
t.penup()
t.forward(50)
t.right(90)
t.pendown()
t.forward(50)
t.right(90)
t.penup()
t.forward(50)
t.pendown()